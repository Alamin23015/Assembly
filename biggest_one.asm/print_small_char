.model small
.stack 100h
.data
.code
main proc
    mov ax,@data
    mov ds,ax
  
    mov ah,1 
     int 21h
    mov bl,al ;First Char
   
    mov ah,1
    int 21h
    mov bh,al   ; 2nd char    
     
    cmp bl,bh 
    jbe show_bl
    mov dl,bh
    mov ah,2
    int 21h
    jmp exit
    
    show_bl:
    mov dl,bl
    mov ah,2
    int 21h
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main
    
